{
  "name": "aem-clientlib-webpack-plugin",
  "version": "1.0.4",
  "description": "A webpack plugin to generate clientlib structure based a clientlib.config.js file",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib --source-maps",
    "watch": "./node_modules/.bin/babel src --out-dir lib --watch --plugins=",
    "lint": "./node_modules/.bin/eslint ./src/**.js",
    "lint:fix": "./node_modules/.bin/eslint ./src/**.js --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "aem",
    "clientlib",
    "webpack",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/samunplugged/aem-clientlib-webpack-plugin.git"
  },
  "author": "Sameer Gupta",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "glob-fs": "^0.1.7"
  },
  "dependencies": {
    "async": "^2.6.0",
    "eslint": "^4.15.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.4"
  }
}
