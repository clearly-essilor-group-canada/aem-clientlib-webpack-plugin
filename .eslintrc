{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": false
    }
},
  "rules": {
    "strict": 0,
    "class-methods-use-this": 0,
    "no-multi-str": 0,
    "object-curly-spacing": 0,
    "padded-blocks": 0,
    "spaced-comment": 0
  }
}